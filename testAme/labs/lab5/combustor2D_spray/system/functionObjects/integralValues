pInlet
{
	type		surfaceFieldValue;
	enabled		yes;
	log		true;
	writeFormat     binary;
	writeControl	runTime;
	writeInterval	0.01;
	regionType	patch;
	writeFields	no;
	name		inlet;
	operation	average;
	fields		(p);
	libs		("libfieldFunctionObjects.so");
}

pOutlet
{
	type		surfaceFieldValue;
	enabled		yes;
	log		true;
	writeFormat     binary;		
	writeControl	runTime;
	writeInterval	0.01;
	regionType	patch;
	writeFields	no;
	name		outlet;
	operation	average;
	fields		(p);
	libs		("libfieldFunctionObjects.so");
}




