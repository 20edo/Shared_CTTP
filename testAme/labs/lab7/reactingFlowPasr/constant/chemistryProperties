FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
	chemistrySolver		EulerImplicit;
	chemistryThermo		psi;
}

chemistry       		on;

initialChemicalTimeStep 	1e-08;

EulerImplicitCoeffs
{
	cTauChem		1;
	equilibriumRateLimiter	on;
}

#include "$FOAM_CASE/constant/reactions"

