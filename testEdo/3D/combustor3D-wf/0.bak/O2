FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    object      O2;
}
// ************************************************************************* //

dimensions          [0 0 0 0 0 0 0];

internalField       uniform 0.233;

boundaryField
{
    region0_to_wallFilmRegion_wallFilmFaces
    {
        type                zeroGradient;
    }
    outlet
    {
        type                zeroGradient;
    }
    inlet
    {
        type                fixedValue;
        value               $internalField;
    }
}

