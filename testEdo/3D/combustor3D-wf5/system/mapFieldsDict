	splitterRight	region0_to_wallFilmRegion_wallFilmFaces
	splitterTop	region0_to_wallFilmRegion_wallFilmFaces
	splitterBottom	region0_to_wallFilmRegion_wallFilmFaces
	splitterRear	region0_to_wallFilmRegion_wallFilmFaces
	outerWalls	region0_to_wallFilmRegion_wallFilmFaces
);

