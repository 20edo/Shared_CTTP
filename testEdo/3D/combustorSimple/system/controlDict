FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         700;

deltaT          1;

writeControl    timeStep;

writeInterval   100;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
	#includeFunc	residuals(p,U,k,epsilon)
	#include	"functionObjects/forces"
	#include	"functionObjects/surfaces"
	// #include 	"functionObjects/boundaryLayer"
	#include 	"functionObjects/integralValues"
	yPlus
	{
    	// Mandatory entries
    	type            yPlus;
    	libs            ("fieldFunctionObjects.so");

    	// Optional (inherited) entries
    	writePrecision  10;
    	writeToFile     true;
    	useUserTime     true;

    	enabled         true;
    	log             true;
    	executeControl  timeStep;
    	executeInterval 1;
    	writeControl    writeTime;
    	writeInterval   -1;
	}
}


}
