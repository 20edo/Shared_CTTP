FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    location        "constant";
    object          combustionProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

combustionModel infinitelyFastChemistry;

active          yes;

semiImplicit no;

infinitelyFastChemistryCoeffs
{
    C               1;
    semiImplicit    yes;
}

PaSRCoeffs
{
    Cmix            1;
}

reaction
{
    type     irreversibleArrheniusReaction;
    reaction "CH4 + 2O2 = CO2 + 2H2O";
    A        5.2e16;
    beta     0;
    Ta       14906;
}

