FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      N2;
}
// ************************************************************************* //

dimensions          [0 0 0 0 0 0 0];

internalField       uniform 0.767;

boundaryField
{
    "(splitter|back|sides)"
    {
        type                zeroGradient;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    inlet
    {
        type                fixedValue;
        value               $internalField;
    }
    fuel_inlet
    {
        type                fixedValue;
        value               uniform 0;
    }


}

