FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "3";
    object      nSurfaceLayers;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    frontAndBack
    {
        type            fixedValue;
        value           uniform 0;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 
0
;
    }
    outlet
    {
        type            fixedValue;
        value           nonuniform List<scalar> 
0
;
    }
    lowerWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    upperWall
    {
        type            fixedValue;
        value           uniform 0;
    }
    body
    {
        type            fixedValue;
        value           uniform 3;
    }
    legs
    {
        type            fixedValue;
        value           uniform 3;
    }
    procBoundary3to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary3to2
    {
        type            processor;
        value           uniform 0;
    }
}


